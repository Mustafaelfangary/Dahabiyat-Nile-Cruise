  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --max-semi-space-size=512\" next dev",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "ts-node --project prisma/tsconfig.json prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:minimal": "tsx prisma/seed-minimal.ts",
    "cleanup:homepage": "tsx scripts/cleanup-homepage.ts",
    "cleanup:complete": "tsx scripts/complete-cleanup.ts",
    "reset:migrations": "tsx scripts/reset-migrations.ts",
    "fresh:start": "npm run reset:migrations && npm run cleanup:complete",
    "postinstall": "prisma generate"
  },
